<div class="container">
    <table class="table table-hover">
        <thead>
            <tr class="head-title">
                <th scope="col">SL No.</th>
                <th scope="col">Book Name</th>
                <th scope="col">Author Name</th>
                <th scope="col">Published On</th>
                <th scope="col">Description</th>
                <th scope="col">Image</th>
            </tr>
        </thead>
        <tbody>
            <% for (var i=0; i< product.length; i++) { %>
            <tr class="table-data" >
                <th scope="row"></th>
                <td><%= product[i].bookName %></td>
                <td><%= product[i].authorName %></td>
                <td><%= product[i].publishedDate %></td>
                <td><%= product[i].description %></td>
                <td><img width="40px;" height="40px;" src="<%= product[i].image %>" alt="Wrong"></td>
            </tr>
            <% } %>
        </tbody>
    </table>
</div>